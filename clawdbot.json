{
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "trustedProxies": ["100.64.0.0/10", "10.0.0.0/8"],
    "auth": {
      "token": "railway-clawdbot-token-2026"
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/data/clawd",
      "model": {
        "primary": "anthropic/claude-opus-4-5",
        "fallbacks": [
          "anthropic/claude-sonnet-4-5",
          "openrouter/anthropic/claude-sonnet-4-5",
          "openai/gpt-4o"
        ]
      },
      "models": {
        "anthropic/claude-opus-4-5": { "alias": "Opus" },
        "anthropic/claude-sonnet-4-5": { "alias": "Sonnet" },
        "openrouter/anthropic/claude-opus-4-5": { "alias": "OR-Opus" },
        "openrouter/anthropic/claude-sonnet-4-5": { "alias": "OR-Sonnet" },
        "openrouter/google/gemini-2.5-pro-preview": { "alias": "Gemini-Pro" },
        "openrouter/google/gemini-2.5-flash-preview": { "alias": "Gemini-Flash" },
        "openrouter/meta-llama/llama-3.3-70b-instruct": { "alias": "Llama-70B" },
        "openrouter/deepseek/deepseek-chat-v3": { "alias": "DeepSeek" },
        "openai/gpt-4o": { "alias": "GPT-4o" },
        "openai/gpt-4o-mini": { "alias": "GPT-4o-mini" },
        "google/gemini-2.0-flash": { "alias": "Gemini" },
        "groq/llama-3.3-70b-versatile": { "alias": "Groq-Llama" },
        "xai/grok-2": { "alias": "Grok" },
        "mistral/mistral-large-latest": { "alias": "Mistral" }
      }
    }
  },
  "skills": {
    "install": {
      "preferBrew": true,
      "nodeManager": "pnpm"
    }
  },
  "env": {
    "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
    "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
    "OPENAI_API_KEY": "${OPENAI_API_KEY}",
    "GEMINI_API_KEY": "${GEMINI_API_KEY}",
    "GROQ_API_KEY": "${GROQ_API_KEY}",
    "XAI_API_KEY": "${XAI_API_KEY}",
    "MISTRAL_API_KEY": "${MISTRAL_API_KEY}"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "dmPolicy": "open",
      "allowFrom": ["*"]
    }
  }
}
